#include<iostream>
using namespace std;

int main()
{
   int n,sum=0,rem;
   cout<<"Enter the number = ";
   cin>>n;
   int a=n;
   while(n!=0){
     rem=n%10;
     sum=sum+(rem*rem*rem);
     n=n/10;
   }
    if(a==sum)
     cout<<"The number is armstrong";
    else
     cout<<"The number is not armstrong";
    return 0;
  }
