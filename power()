/*
Write a C++ Program to raise any number X to power N.
Hereâ€™s simple C++ Program to find power of a Number using pow( ) function in C++ Programming Language.


*/

#include <iostream>
#include<math.h>

using namespace std;

int main()
{
    int a,b,result;
    cout<<"Enter value of a :: ";
    cin>>a;
    cout<<"Enter value of b :: ";
    cin>>b;
    result=pow(a,b);
    cout<<"The power of numbers ["<<"a"<<"^"<<"b]"<<result<<endl;

 


    return 0;
}
